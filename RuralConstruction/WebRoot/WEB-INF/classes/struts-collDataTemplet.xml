<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.1//EN" "http://struts.apache.org/dtds/struts-2.1.dtd">
<struts>
 
	<package name="invpro" namespace="/" extends="struts-default,json-default">
		<action name="invespro_*" class="InvesProTreeAction" method="{1}" >
			<result type="json">
                <param name="root">dataMap</param>
			</result>
		</action>		
	</package>
	
	<package name="invesObj" namespace="/" extends="struts-default,json-default">
	
        <action name="invesObj_*" class="InvesObjAction" method="{1}" >
	        <result type="json">
				<param name="root">dataMap</param>
			</result>
		</action>
	</package>
</struts>    

