<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.1//EN" "http://struts.apache.org/dtds/struts-2.1.dtd">
<struts>
	<constant name="struts.i18n.encoding" value="UTF-8"/>
	<package name="district" namespace="/" extends="struts-default,json-default">
	
        <action name="District_*" class="DistrictAction" method="{1}" >
	        <result type="json">
				<param name="root">dataMap</param>
			</result>
		</action>
	</package>
	<package name="district1" namespace="/" extends="struts-default,json-default">
	
        <action name="District1_*" class="DistrictAction1" method="{1}" >
	        <result type="json">
				<param name="root">dataMap</param>
			</result>
		</action>
	</package>
	
	<package name="admitree" namespace="/" extends="struts-default,json-default">
	
        <action name="AdmiTree_*" class="AdmiTreeAction" method="{1}" >
	        <result type="json">
				<param name="root">dataMap</param>
			</result>
		</action>
	</package>
	
</struts>    
