<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.1//EN" "http://struts.apache.org/dtds/struts-2.1.dtd">
<struts>
	<constant name="struts.i18n.encoding" value="UTF-8"/>
	<package name="ExpertOLGra" namespace="/" extends="struts-default,json-default">
	    <action name="expertOLGra_*" class="ExpertOLGraAction" method="{1}" >
	        <result type="json">
				<param name="root">dataMap</param>
			</result>
		</action>
		<action name="evalContObjAttri_*" class="EvalContObjAttriAction" method="{1}" >
	        <result type="json">
				<param name="root">dataMap</param>
			</result>
		</action>
		<action name="evalContFileData_*" class="EvalContFileDataAction" method="{1}" >
	        <result type="json">
				<param name="root">dataMap</param>
			</result>
		</action>
	</package>
</struts>    